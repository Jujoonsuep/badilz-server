<%- include('../../partials/header.ejs') %>

  <!-- Page Wrapper -->
  <div id="wrapper">

    <%- include('../../partials/sidebar.ejs') %>

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

        <%- include('../../partials/navbar.ejs') %>

        <!-- Begin Page Content -->
        <div class="container-fluid">

          <!-- Page Heading -->
          <h1 class="h3 mb-4 text-gray-800">Portofolio</h1>

          <!-- Page Content -->
           <!-- Content Row -->
           <div class="row">

            <!-- Earnings (Monthly) Card Example -->
            <div class="col-xl-6 col-md-6 mb-6">
              <div class="card border-left-grey shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-md-center font-weight-bold text-primary text-uppercase mb-1">
                        <a href="/admin/portofolio/video">Portofolio Video</a>
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800 text-center"><%= video.length %></div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-play-circle fa-2x text-gray-300"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Earnings (Monthly) Card Example -->
            <div class="col-xl-6 col-md-6 mb-6">
              <div class="card border-left-grey shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-md-center font-weight-bold text-success text-uppercase mb-1">
                        <a href="/admin/portofolio/image">Portofolio Image</a>
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800 text-center"><%= image.length %></div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-camera-retro fa-2x text-gray-300"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Earnings (Monthly) Card Example -->

          </div>

          <!-- Content Row -->

        </div>
        <!-- /.container-fluid -->

      </div>
      <!-- End of Main Content -->

      <%- include('../../partials/logout.ejs') %>

      <%- include('../../partials/footer.ejs') %>

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->
  
<%- include('../../partials/js.ejs') %>
